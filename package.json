{
  "name": "kentcdodds.com",
  "version": "1.0.0",
  "engines": {
    "node": "16",
    "npm": "7"
  },
  "description": "A remix version of kentcdodds.com",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kentcdodds/remix-kentcdodds.com.git"
  },
  "keywords": [],
  "author": "Kent C. Dodds <me@kentcdodds.com> (https://kentcdodds.com/)",
  "license": "GPL-3.0-only",
  "bugs": {
    "url": "https://github.com/kentcdodds/remix-kentcdodds.com/issues"
  },
  "homepage": "https://github.com/kentcdodds/remix-kentcdodds.com#readme",
  "scripts": {
    "postinstall": "patch-package --patch-dir other/patches",
    "prepare": "husky install",
    "css:watch": "npm run build:css -- --w",
    "clean": "rimraf ./node_modules/.cache ./server/dist ./build ./public/build \"./app/styles/**/*.css\"",
    "prebuild": "npm run clean && echo All clean ✨",
    "build:css": "postcss styles/**/*.css --base styles --dir app/styles",
    "build:css:prod": "npm run build:css -- --env production",
    "build:remix": "cross-env NODE_ENV=production dotenv -e .env remix build",
    "build": "npm run build:css:prod && npm run build:remix",
    "dev": "pm2-dev ./other/pm2.config.js",
    "runfile": "node ./other/runfile.js",
    "start": "cross-env NODE_ENV=production node --require ./node_modules/dotenv/config --require ./other/tracing.js --experimental-wasm-threads ./index.js",
    "start:mocks": "cross-env NODE_ENV=production node --require ./mocks --require ./node_modules/dotenv/config --require ./other/tracing.js --experimental-wasm-threads ./index.js",
    "curl:blog": "curl --silent http://localhost:$PORT/blog > /dev/null",
    "prime-cache:prod": "PORT=4001 npx start-server-and-test start http-get://localhost:4001/build-info curl:blog && echo cache primed ⚡️",
    "prime-cache:mocks": "PORT=4001 npx start-server-and-test start:mocks http-get://localhost:4001/build-info curl:blog && echo cache primed ⚡️",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test:e2e:dev": "cross-env RUNNING_E2E=true ENABLE_TEST_ROUTES=true start-server-and-test dev http-get://localhost:3000/build-info cy:open",
    "test:e2e:run": "cross-env RUNNING_E2E=true PORT=8811 start-server-and-test start:mocks http-get://localhost:8811/build-info cy:run",
    "test": "jest",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/.eslintcache --ext js,jsx,ts,tsx .",
    "format": "prettier --write \"**/*.+(js|jsx|json|yml|yaml|css|less|scss|ts|tsx|md|gql|graphql|mdx|vue)\"",
    "typecheck": "tsc -b",
    "validate": "./other/validate",
    "setup": "npm install && docker compose up -d && prisma migrate reset --force && npm run validate && npm run prime-cache:mocks && npm run test:e2e:run"
  },
